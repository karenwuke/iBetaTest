<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ke.ibetatesting">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".view.choosingActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".view.managerLoginActivity" />
        <activity android:name=".view.testerLoginActivity" />
        <activity
            android:name=".view.testerMainActivity"
            android:label="                  TESTER    MENU" />
        <activity
            android:name=".view.managerMainActivity"
            android:label="                  MANAGER   MENU" />
        <activity android:name=".view.signupActivity" />
        <activity android:name=".view.analyzeActivity" />
        <activity android:name=".view.addtestdata_location2Activity" />
        <activity android:name=".view.datamanage" />

        <activity android:name=".view.LocationDataAnalyze" />
